        "+ ",
        "       y = \"Coverage Change\",",
        "+ ",
        "       shape = \"Point Type\",",
        "+ ",
        "       color = \"Point Type\") +",
        "+ ",
        "  guides(color = guide_legend(override.aes = list(shape = 1)), shape = guide_legend(override.aes = list(color = \"black\"))) ",
